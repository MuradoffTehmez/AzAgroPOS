{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "File": {
      "Path": "Logs/production-log-.txt",
      "MinLevel": "Warning"
    }
  },
  "Environment": "Production",
  "Security": {
    "EnforceHttps": true,
    "RequireStrongPasswords": true
  }
}
