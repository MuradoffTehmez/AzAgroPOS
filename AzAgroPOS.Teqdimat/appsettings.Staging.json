{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    },
    "File": {
      "Path": "Logs/staging-log-.txt",
      "MinLevel": "Information"
    }
  },
  "Environment": "Staging",
  "Security": {
    "EnforceHttps": true,
    "RequireStrongPasswords": true
  }
}
